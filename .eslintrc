{
	"globals": {},
	"env": {
		"es2021": true,
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"parser": "babel-eslint",
		"allowImportExportEverywhere": true
	},
	"rules": {
		"semi": [
			"error",
			"always"
		],
		"quotes": [
			"error",
			"double"
		]
	}
}